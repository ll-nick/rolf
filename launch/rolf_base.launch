<launch>
    <env name="ROS_IP" value="192.168.0.99"/>
    <env name="ROS_MASTER_URI" value="http://raspberrypi:11311"/>

    <!-- Camera -->
    <include file="$(find raspicam_node)/launch/camerav2_1280x720.launch"/>

    <!-- Motor Driver -->
    <node pkg="rolf_pkg" type="motor_driver_node.py" name="motor_driver" output="screen"/>
</launch>